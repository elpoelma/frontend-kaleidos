<Auk::Modal>
  <Auk::Modal::Header
    @title={{this.modalTitle}}
    @onClose={{@onCancel}} />
  <Auk::Modal::Body>

    <Auk::CheckboxList>
      {{#each @attachmentRows as |attachmentRow|}}
        <Auk::Checkbox
          @checked={{includes this.attachmentsSelection attachmentRow}}
          {{on 'input' (fn this.toggleSelection attachmentRow)}} />
      {{/each}}
    </Auk::CheckboxList>

  </Auk::Modal::Body>
  <Auk::Modal::Footer></Auk::Modal::Footer>
</Auk::Modal>