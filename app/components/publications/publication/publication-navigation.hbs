<Auk::Tabs @reversed={{true}}>
  <Auk::Tab
    @isHierarchicalBack={{true}}
    @route="publications.index"
    data-test-publication-nav-go-back />
  <Auk::Tab
    @route="publications.publication.case"
    data-test-publication-nav-case
  >
    {{t "publicationSection.publication-case"}}
  </Auk::Tab>
  <Auk::Tab
    @route="publications.publication.documents"
    @counter={{this.documentsCount}}
    data-test-publication-nav-documents
  >
    {{t "publicationSection.decisions"}}
  </Auk::Tab>
  <Auk::Tab
    @route="publications.publication.translations"
    data-test-publication-nav-translations
  >
    {{t "publicationSection.translation"}}
  </Auk::Tab>
  <Auk::Tab
    @route="publications.publication.proofs"
    data-test-publication-nav-publishpreview
  >
    {{t "publicationSection.proof"}}
  </Auk::Tab>
  <Auk::Tab
    @route="publications.publication.publication"
    data-test-publication-nav-publication
  >
    {{t "publicationSection.publication"}}
  </Auk::Tab>
</Auk::Tabs>