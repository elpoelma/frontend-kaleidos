<PowerSelect
  @selected={{@selected}}
  @options={{@options}}
  @placeholder={{@placeholder}}
  @noMatchesMessage={{@noMatchesMessage}}
  @onopen={{this.onOpen}}
  @oninput={{this.onInput}}
  @search={{this.search}}
  @onchange={{@onChange}}
  ...attributes
  as |option|>
  {{yield option}}
</PowerSelect>