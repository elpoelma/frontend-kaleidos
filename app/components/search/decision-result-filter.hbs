<Auk::Label>{{t "result-decision"}}</Auk::Label>
{{#if this.loadDecisionResultCodes.isRunning}}
  <Auk::Loader />
{{else}}
  <Auk::CheckboxTree
    @label={{capitalize (t "all-results")}}
    @items={{this.decisionResultCodes}}
    @selectedItems={{this.selectedDecisionResultCodes}}
    @didUpdate={{this.onChangeDecisionResultCodes}}
  />
{{/if}}
