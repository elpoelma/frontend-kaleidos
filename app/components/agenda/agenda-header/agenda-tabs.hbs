<Auk::Tabs @reversed={{true}}>
  <Auk::Tab @route="agendas" @isHierarchicalBack={{true}} />
  <Auk::Tab
    @route="agenda.agendaitems.index"
  >
    {{t "overview"}}
  </Auk::Tab>
  {{#if this.firstAgendaitem}}
    <Auk::Tab
      @route="agenda.agendaitems.agendaitem"
      @models={{this.modelsForDetailRoute}}
    >
      {{t "detail"}}
    </Auk::Tab>
  {{/if}}
  {{#if this.currentSession.isEditor}}
    <Auk::Tab @route="agenda.compare">
      {{t "compare"}}
    </Auk::Tab>
  {{/if}}
  <Auk::Tab @route="agenda.documents">
    {{t "documents"}}
  </Auk::Tab>
</Auk::Tabs>