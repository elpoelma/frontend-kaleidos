{{page-title (t "subcases")}}
{{subcases/subcases-overview-header
  model=model
  isShowingProcess=isShowingProcess
  backLink=(action "backLinkAction")
  refresh=(action "refresh")
}}
<div class="auk-scroll-wrapper__body">
  {{#if (gt (await model.subcases.length) 0)}}

    <Subcases::SubcasesProcess @subcases={{await model.subcases}}/>

  {{else}}
    <div class="auk-u-m-8">
      {{web-components/vl-alert message=(t "no-subcases")}}
    </div>
  {{/if}}
</div>