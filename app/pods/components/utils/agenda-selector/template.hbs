{{#if (not multiSelect)}}
  {{#power-select
    options=agendas
    selected=selectedAgenda
    disabled=readOnly
    searchEnabled=false
    placeholder=placeholder
    onchange=(action "selectModel")
  as |model|
  }}
    {{model-property-to-show model propertyToShow}}
  {{/power-select}}
{{else}}
  {{#power-select-multiple
    options=agendas
    searchEnabled=false
    selected=selectedAgenda
    disabled=readOnly
    placeholder=placeholder
    onchange=(action "selectModel")
  as |model|
  }}
    {{model-property-to-show model propertyToShow}}
  {{/power-select-multiple}}
{{/if}}